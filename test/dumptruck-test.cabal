name:                dumptruck-test
version:             0.1.0.0
synopsis:            Test web app for the DumpTruck library
-- description:         
license:             BSD3
license-file:        LICENSE
author:              Steven Smith
maintainer:          guntir@teamclerks.net
-- copyright:           
category:            Web
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable dumptruck-test
  main-is:             Main.hs
  -- other-modules:       
  other-extensions:    OverloadedStrings
  build-depends:       base >=4.7 && <4.8
                     , transformers >=0.3 && <0.4
                     , aeson >=0.7 && <0.8
                     , attoparsec >=0.11 && <0.12
                     , bytestring >=0.10 && <0.11
                     , containers >=0.5 && <0.6
                     , text >=1.1 && <1.2
                     , directory >=1.2 && <1.3
                     , dumptruck
                     , warp
                     , wai
  hs-source-dirs:      src
  default-language:    Haskell2010
